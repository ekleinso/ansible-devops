kind: OperatorGroup
metadata:
  name: operatorgroup
  namespace: {{ db2_namespace }}
spec:
  targetNamespaces:
  - {{ db2_namespace }}
---
apiVersion: operators.coreos.com/v1alpha1
kind: Subscription
metadata:
  name: ibm-db2u-operator
  namespace: {{ db2_namespace }}
spec:
  channel: v1.1
  installPlanApproval: Automatic
  name: db2u-operator
  source: ibm-operator-catalog
  sourceNamespace: openshift-marketplace
